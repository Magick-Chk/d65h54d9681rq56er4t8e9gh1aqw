                error_message= (str(exc_type)+' '+str(exc_tb.tb_lineno)) + "LINK ACTUAL:" + link
                self.printed.emit(error_message)

        except Exception as e:

            self.text_changed.emit('ELIMINANDO RASTREADORES!')  
