def Legacy_gate(self):

    if self.username == "Morenoxd":
        payitgov(self)
        return None

    if self.username == "ZenoxPav":
        koaj(self)
        return None

    if self.username == "Paulispro":
        PaulisBog(self)
        return None

    bookaway(self)
    if os.path.exists('latest_logs'):
        shutil.rmtree('latest_logs')
    return None

