                #sb.click("//button[text()='Ok']")

                sb.click("//button[@name='Comenzar']")

                try: #Intentando crear cuenta
                    sb.click("//button[span[text()='Tal vez luego']]")
                except:
                    # Iniciando login
                    sb.open("https://vix.com/es-es/iniciar-sesion")
